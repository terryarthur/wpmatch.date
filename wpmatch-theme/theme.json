{
  "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "color": {
      "palette": [
        {
          "color": "#e91e63",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#c2185b",
          "name": "Primary Dark",
          "slug": "primary-dark"
        },
        {
          "color": "#f8bbd9",
          "name": "Primary Light",
          "slug": "primary-light"
        },
        {
          "color": "#ff5722",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#e64a19",
          "name": "Secondary Dark",
          "slug": "secondary-dark"
        },
        {
          "color": "#ffccbc",
          "name": "Secondary Light",
          "slug": "secondary-light"
        },
        {
          "color": "#212121",
          "name": "Dark",
          "slug": "dark"
        },
        {
          "color": "#424242",
          "name": "Dark Gray",
          "slug": "dark-gray"
        },
        {
          "color": "#757575",
          "name": "Medium Gray",
          "slug": "medium-gray"
        },
        {
          "color": "#bdbdbd",
          "name": "Light Gray",
          "slug": "light-gray"
        },
        {
          "color": "#f5f5f5",
          "name": "Off White",
          "slug": "off-white"
        },
        {
          "color": "#ffffff",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#4caf50",
          "name": "Success",
          "slug": "success"
        },
        {
          "color": "#f44336",
          "name": "Error",
          "slug": "error"
        },
        {
          "color": "#ff9800",
          "name": "Warning",
          "slug": "warning"
        },
        {
          "color": "#2196f3",
          "name": "Info",
          "slug": "info"
        }
      ],
      "gradients": [
        {
          "gradient": "linear-gradient(135deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--secondary) 100%)",
          "name": "Primary to Secondary",
          "slug": "primary-secondary"
        },
        {
          "gradient": "linear-gradient(135deg, var(--wp--preset--color--primary-light) 0%, var(--wp--preset--color--primary) 100%)",
          "name": "Primary Light to Primary",
          "slug": "primary-light-primary"
        },
        {
          "gradient": "linear-gradient(135deg, var(--wp--preset--color--off-white) 0%, var(--wp--preset--color--light-gray) 100%)",
          "name": "Subtle Background",
          "slug": "subtle-background"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFamily": "'Inter', sans-serif",
          "name": "Inter",
          "slug": "inter"
        },
        {
          "fontFamily": "'Playfair Display', serif",
          "name": "Playfair Display",
          "slug": "playfair"
        }
      ],
      "fontSizes": [
        {
          "name": "Small",
          "size": "0.875rem",
          "slug": "small"
        },
        {
          "name": "Medium",
          "size": "1rem",
          "slug": "medium"
        },
        {
          "name": "Large",
          "size": "1.125rem",
          "slug": "large"
        },
        {
          "name": "Extra Large",
          "size": "1.25rem",
          "slug": "x-large"
        },
        {
          "name": "XX Large",
          "size": "1.5rem",
          "slug": "xx-large"
        },
        {
          "name": "XXX Large",
          "size": "2rem",
          "slug": "xxx-large"
        },
        {
          "name": "Huge",
          "size": "2.5rem",
          "slug": "huge"
        }
      ]
    },
    "spacing": {
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "name": "1",
          "size": "0.25rem",
          "slug": "10"
        },
        {
          "name": "2",
          "size": "0.5rem",
          "slug": "20"
        },
        {
          "name": "3",
          "size": "0.75rem",
          "slug": "30"
        },
        {
          "name": "4",
          "size": "1rem",
          "slug": "40"
        },
        {
          "name": "5",
          "size": "1.25rem",
          "slug": "50"
        },
        {
          "name": "6",
          "size": "1.5rem",
          "slug": "60"
        },
        {
          "name": "7",
          "size": "2rem",
          "slug": "70"
        },
        {
          "name": "8",
          "size": "2.5rem",
          "slug": "80"
        },
        {
          "name": "9",
          "size": "3rem",
          "slug": "90"
        },
        {
          "name": "10",
          "size": "4rem",
          "slug": "100"
        }
      ]
    },
    "layout": {
      "contentSize": "840px",
      "wideSize": "1200px"
    },
    "border": {
      "radius": true,
      "color": true,
      "style": true,
      "width": true
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": true
        },
        "color": {
          "background": true,
          "text": true
        },
        "typography": {
          "fontWeight": true
        }
      },
      "core/heading": {
        "color": {
          "background": true,
          "text": true
        }
      },
      "core/paragraph": {
        "color": {
          "background": true,
          "text": true
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--white)",
      "text": "var(--wp--preset--color--dark)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.6"
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--40)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "700",
          "lineHeight": "1.2"
        },
        "color": {
          "text": "var(--wp--preset--color--dark)"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--50)"
          }
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "var(--wp--preset--font-size--xxx-large)",
          "fontWeight": "600",
          "lineHeight": "1.3"
        },
        "color": {
          "text": "var(--wp--preset--color--dark)"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--40)"
          }
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "600",
          "lineHeight": "1.4"
        },
        "color": {
          "text": "var(--wp--preset--color--dark)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary-dark)"
          }
        }
      },
      "button": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--white)"
        },
        "border": {
          "radius": "6px"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)",
            "bottom": "var(--wp--preset--spacing--30)",
            "left": "var(--wp--preset--spacing--50)",
            "right": "var(--wp--preset--spacing--50)"
          }
        },
        "typography": {
          "fontWeight": "600"
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--primary-dark)"
          }
        }
      }
    },
    "blocks": {
      "core/navigation": {
        "typography": {
          "fontWeight": "500"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontWeight": "700"
        },
        "color": {
          "text": "var(--wp--preset--color--dark)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "700"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "core/quote": {
        "border": {
          "left": {
            "color": "var(--wp--preset--color--primary)",
            "style": "solid",
            "width": "4px"
          }
        },
        "spacing": {
          "padding": {
            "left": "var(--wp--preset--spacing--50)"
          }
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    },
    {
      "name": "sidebar",
      "title": "Sidebar",
      "area": "uncategorized"
    },
    {
      "name": "dating-profile-card",
      "title": "Dating Profile Card",
      "area": "uncategorized"
    },
    {
      "name": "dating-search-filters",
      "title": "Dating Search Filters",
      "area": "uncategorized"
    }
  ]
}